@font-face {
	font-family: 'digip';
	src:url('../fonts/digip.eot?-rh1740');
	src:url('../fonts/digip.eot?#iefix-rh1740') format('embedded-opentype'),
		url('../fonts/digip.woff?-rh1740') format('woff'),
		url('../fonts/digip.ttf?-rh1740') format('truetype'),
		url('../fonts/digip.svg?-rh1740#digip') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'digip';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

